{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "js/jquery.js", "js/inject.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "img/icons/icon128.png",
      "16": "img/icons/icon16.png"
   },
   "manifest_version": 2,
   "name": "__MSG_name__",
   "permissions": [ "tabs", "storage", "*://*/*" ],
   "version": "1.0"
}
